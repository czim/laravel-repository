{
    "name": "czim/laravel-repository",
    "description": "Repository for Laravel (inspired by and indebted to Bosnadev/Repositories)",
    "keywords": [
        "laravel",
        "repository",
        "repositories",
        "eloquent",
        "database",
        "criteria"
    ],
    "homepage": "https://github.com/czim/laravel-repository",
    "license": "MIT",
    "authors": [
        {
            "name": "Coen Zimmerman",
            "email": "coen.zimmerman@endeavour.nl",
            "homepage": "https://endeavour.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^8.1",
        "illuminate/support": "^9.0",
        "illuminate/database": "^9.0",
        "myclabs/php-enum": "^1.7"
    },
    "require-dev": {
        "astrotomic/laravel-translatable": "^11.10",
        "czim/laravel-listify": "^2.0",
        "orchestra/testbench": "^7.5",
        "nunomaduro/larastan": "^2.2",
        "phpstan/phpstan-mockery": "^1.1",
        "phpstan/phpstan-phpunit": "^1.1",
        "phpunit/phpunit" : "^9.5",
        "watson/rememberable": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Czim\\Repository\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\Repository\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Czim\\Repository\\RepositoryServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
